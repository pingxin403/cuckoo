<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude generated protobuf code -->
    <Match>
        <Class name="~com\.myorg\.api\.v1\..*"/>
    </Match>
    <Match>
        <Class name="~com\.pingxin403\.api\.v1\..*"/>
    </Match>
    <Match>
        <Class name="~com\.pingxin403\.cuckoo\..*\.v1\..*"/>
    </Match>
    
    <!-- Exclude generated code -->
    <Match>
        <Class name="~.*\.gen\..*"/>
    </Match>
    <Match>
        <Class name="~.*\.generated\..*"/>
    </Match>
    <Match>
        <Package name="~.*\.proto\..*"/>
    </Match>
    
    <!-- Exclude test code from certain checks -->
    <Match>
        <Class name="~.*Test"/>
        <Bug pattern="DM_DEFAULT_ENCODING"/>
    </Match>
    
    <!-- Exclude Spring Boot application class -->
    <Match>
        <Class name="~.*Application"/>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
    
    <!-- Allow serialization without serialVersionUID in DTOs -->
    <Match>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
    
    <!-- Exclude protobuf generated code issues -->
    <Match>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
</FindBugsFilter>
